			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.soffid.zkib</groupId>
			<artifactId>zkdb</artifactId>
			<version>1.5</version>
		</dependency>
		<dependency>
			<groupId>com.soffid.iam.addon.recertification</groupId>
			<artifactId>recertification-common</artifactId>
