<?xml version="1.0" encoding="UTF-8"?>

<process>
	<tag>${pom.version}</tag>
	<roles>
		<initiator>
			<user name="-"/>
		</initiator>
		<supervisor>
			<role name="SEU_ADMIN" />
			<role name="SOFFID_ADMIN" />
		</supervisor>

		<observer>
			<role name="SEU_ADMIN" />
			<role name="SOFFID_ADMIN" />
		</observer>
	</roles>

	<task name="User permissions review">
		<file path="ui/userReview.zul" />
	</task>
	
	<task name="User permissions aproval">
		<file path="ui/bossReview.zul" />
	</task>
	
	<upgrade>
		<process>
			<tag>${project.version}</tag>
		</process>
	</upgrade>
</process>
